class Solution {
    public int[] twoSum(int[] nums, int t) {
        Map m=new HashMap<>();
       
        for(int i=0;i<nums.length;i++){
            int bal=t-nums[i];

             if(m.containsKey(bal)){
                return new int[]{(int)m.get(bal),i};

             }

               m.put(nums[i],i);
             
        }

        
        return new int[]{};

        
       

        
    }
}
